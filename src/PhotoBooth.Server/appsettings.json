{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "File": {
      "Path": "logs/photobooth.log",
      "Append": false
    }
  },
  "AllowedHosts": "*",
  "Camera": {
    "UseMock": false,
    "DeviceIndex": 0,
    "CaptureLatencyMs": 100,
    "FramesToSkip": 5,
    "FlipVertical": true,
    "PixelOrder": "ARGB",
    "JpegQuality": 90
  },
  "Capture": {
    "CountdownDurationMs": 3000
  },
  "Input": {
    "EnableKeyboard": false
  },
  "PhotoStorage": {
    "Path": ""
  }
}
